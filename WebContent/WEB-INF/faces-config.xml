<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	
	<navigation-rule>
		<from-view-id>/indexLoginUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>indexConnected</from-outcome>
			<to-view-id>/user/indexConnected.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/user/indexConnected.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/indexLoginUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		<navigation-rule>
		<from-view-id>/user/indexRequest.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>indexConnected</from-outcome>
			<to-view-id>/user/indexConnected.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
